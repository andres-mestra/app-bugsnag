{
  "name": "mock-test",
  "productName": "app-test",
  "version": "1.0.0",
  "description": "My Electron application description",
  "repository": "https://github.com/ticketplushq/totem-app",
  "main": ".webpack/electron/index.js",
  "scripts": {
    "dev": "concurrently \"npm run vite:dev\" \"npm run electron:dev\"",
    "build": "npm run vite:build && npm run electron:build",
    "build:test": "npm run vite:test && npm run electron:build:test",
    "package": "npm run build && npm run electron:package",
    "make": "npm run build && npm run electron:make",
    "vite:dev": "vite --mode development",
    "vite:test": "vite build --mode test",
    "vite:build": "vite build --mode production",
    "electron:dev": "npm run electron:build:dev && npm run electron:start",
    "electron:start": "env-cmd -e dev electron-forge start",
    "electron:build": "env-cmd -e production webpack --config webpack.main.config.js",
    "electron:build:dev": "env-cmd -e dev webpack --config webpack.main.config.js",
    "electron:build:test": "env-cmd -e test webpack --config webpack.main.config.js",
    "electron:package": "env-cmd -e production electron-forge package",
    "electron:make": "env-cmd -e production electron-forge make",
    "electron:rebuild": "electron-rebuild",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [
    "Electron",
    "Totem-app"
  ],
  "author": {
    "name": "Ticketplushq"
  },
  "license": "MIT",
  "devDependencies": {
    "@codeceptjs/configure": "^0.8.0",
    "@codeceptjs/examples": "^1.2.1",
    "@codeceptjs/mock-request": "^0.3.1",
    "@codeceptjs/ui": "^0.4.6",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.63",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.2",
    "@vitejs/plugin-react": "^1.2.0",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "dotenv-webpack": "^7.1.0",
    "electron": "^18.0.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "jsdom": "^19.0.0",
    "node-loader": "^2.0.0",
    "prettier": "2.5.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "4.5.4",
    "vite": "2.8.6",
    "vite-plugin-bugsnag": "^2.0.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@bugsnag/js": "^7.16.2",
    "@bugsnag/plugin-react": "^7.16.2",
    "electron-squirrel-startup": "^1.0.0",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
